{
  "name": "packagesmith.formats.json",
  "version": "0.0.0-development",
  "description": "",
  "homepage": "http://keithamus.github.io/packagesmith.formats.json",
  "bugs": "http://github.com/keithamus/packagesmith.formats.json/issues",
  "license": "MIT",
  "author": "Keith Cirkel (http://keithcirkel.co.uk)",
  "files": [
    "lib/"
  ],
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/packagesmith/packagesmith.formats.json.git"
  },
  "scripts": {
    "lint": "eslint src test --ignore-path .gitignore",
    "prepublish": "babel $npm_package_directories_src -d $npm_package_directories_lib",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "pretest": "npm run lint",
    "test": "mocha --recursive test/ --compilers js:babel-register",
    "watch": "npm run prepublish -- -w"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "npm t"
    }
  },
  "babel": {
    "compact": false,
    "ignore": "node_modules",
    "presets": [
      "es2015",
      "stage-2"
    ],
    "retainLines": true,
    "sourceMaps": "inline"
  },
  "eslintConfig": {
    "extends": "strict"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "eslint": "^3.8.0",
    "eslint-config-strict": "^8.5.0",
    "eslint-plugin-filenames": "^0.2.0",
    "ghooks": "^1.0.3",
    "mocha": "^2.4.5",
    "semantic-release": "^4.3.5",
    "travis-after-all": "^1.4.4",
    "validate-commit-msg": "^2.4.1"
  }
}
